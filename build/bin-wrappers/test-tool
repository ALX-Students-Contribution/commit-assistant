#!/bin/sh

# wrap-for-bin.sh: Template for git executable wrapper scripts
# to run test suite against sandbox, but with only bindir-installed
# executables in PATH.  The Makefile copies this into various
# files in bin-wrappers, substituting
# /home/user/Documents/ALX/commit-assistant/build and t/helper/test-tool.

GIT_EXEC_PATH='/home/user/Documents/ALX/commit-assistant/build'
if test -n "$NO_SET_GIT_TEMPLATE_DIR"
then
	unset GIT_TEMPLATE_DIR
else
	GIT_TEMPLATE_DIR='/home/user/Documents/ALX/commit-assistant/build/templates/blt'
	export GIT_TEMPLATE_DIR
fi
GITPERLLIB='/home/user/Documents/ALX/commit-assistant/build/perl/build/lib'"${GITPERLLIB:+:$GITPERLLIB}"
GIT_TEXTDOMAINDIR='/home/user/Documents/ALX/commit-assistant/build/po/build/locale'
PATH='/home/user/Documents/ALX/commit-assistant/build/bin-wrappers:'"$PATH"

export GIT_EXEC_PATH GITPERLLIB PATH GIT_TEXTDOMAINDIR

case "$GIT_DEBUGGER" in
'')
	exec "${GIT_EXEC_PATH}/t/helper/test-tool" "$@"
	;;
1)
	unset GIT_DEBUGGER
	exec gdb --args "${GIT_EXEC_PATH}/t/helper/test-tool" "$@"
	;;
*)
	GIT_DEBUGGER_ARGS="$GIT_DEBUGGER"
	unset GIT_DEBUGGER
	exec ${GIT_DEBUGGER_ARGS} "${GIT_EXEC_PATH}/t/helper/test-tool" "$@"
	;;
esac
