public class Beer
{
	int special;
	public static void main(String RIGHT[])
	{
		String s=" ";
		for(int x = 99; x > 0; x--)
		{
			System.out.print(x + " bottles of beer on the wall "
				+ x + " bottles of beer\n" // ChangeMe
				+ "Take one down, pass it around, " + (x - 1)
